<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>


<HBox xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.keringo.HelloController">
   <children>
      <Pane prefHeight="600.0" prefWidth="600.0" style="-fx-background-color: white; -fx-border-color: lightgrey;" />
      <VBox alignment="TOP_CENTER" prefWidth="100.0" spacing="5.0">
         <children>
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Start" />
            <Slider fx:id="sdSpeed" majorTickUnit="5.0" max="11.0" min="1.0" minorTickCount="4" value="1.0" />
            <Label fx:id="lbSpeed" text="Speed: 1" />
            <Slider fx:id="sdRadius" max="300.0" min="32.0" value="200.0" />
            <Label fx:id="lbRadius" text="Radius: 200" />
            <ImageView fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@ball1.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@ball2.png" />
               </image>
            </ImageView>
         </children>
         <HBox.margin>
            <Insets bottom="5.0" left="5.0" />
         </HBox.margin>
      </VBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</HBox>
